{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"noImplicitAny": true,
		"declaration": false,
		"removeComments": false,
		"noLib": false,
		"preserveConstEnums": true,
		"sourceMap": false,
		"moduleResolution": "node",
		"isolatedModules": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"suppressImplicitAnyIndexErrors": true
	},
	"exclude": [
		"node_modules"
	]
}
